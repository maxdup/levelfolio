<!doctype html>
<html lang="en" ng-app="folioApp" ng-strict-di ng-controller="RootController">
<head>
  <meta charset="utf-8">
  <title>Maxime Dupuis</title>
  <base href="/"/>
</head>

<body >
  <!-- Map panoramas -->
  <map-panorama ng-model="current_panorama" focus="focus360">
  </map-panorama>

  <!-- Page content -->
  <div class="container" ng-hide="focus360">
    <div class="page-margin" ng-click="focus360 = true"></div>
    <div class="page-centering">
      <div class="page-background"></div>
      <div class="page">
        <!-- Navigation -->
        <navbar></navbar>
        <div class="content">
          <div class="scrollbox">
            <ui-view ng-class="{ reverse: reverse}"
                 class="scrollbox-padded view"></ui-view>
          </div>
        </div>
      </div>
    </div>
  </div>
</body>
</html>
